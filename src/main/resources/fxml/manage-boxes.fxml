<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nisbeterik.chunkr.controller.ManageBoxesController">
   <children>
      <ListView fx:id="leitnerboxListView" layoutX="200.0" layoutY="94.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="backToHomePageButton" layoutX="26.0" layoutY="36.0" mnemonicParsing="false" onMouseClicked="#pressHomePageButton" text="Back to Home Page" />
      <Button fx:id="deleteBoxButton" layoutX="125.0" layoutY="304.0" mnemonicParsing="false" onMouseClicked="#pressDeleteBoxButton" prefHeight="55.0" prefWidth="151.0" text="Delete box" />
      <Button fx:id="manageChunksButton" layoutX="325.0" layoutY="304.0" mnemonicParsing="false" onMouseClicked="#pressManageChunks" prefHeight="55.0" prefWidth="151.0" text="Manage Chunks" />
      <Label layoutX="237.0" layoutY="53.0" text="Click on box to manage" />
      <ImageView fitHeight="75.0" fitWidth="97.0" layoutX="476.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../chunkr-icon-transparent.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
